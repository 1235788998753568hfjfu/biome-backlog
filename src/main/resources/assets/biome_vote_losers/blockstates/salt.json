{
  "multipart": [
    {
      "apply": {
        "model": "biome_vote_losers:block/salt_dot"
      },
      "when": {
        "OR": [
          {
            "east": "none",
            "north": "none",
            "south": "none",
            "west": "none"
          },
          {
            "east": "side|up",
            "north": "side|up"
          },
          {
            "east": "side|up",
            "south": "side|up"
          },
          {
            "south": "side|up",
            "west": "side|up"
          },
          {
            "north": "side|up",
            "west": "side|up"
          }
        ]
      }
    },
    {
      "apply": {
        "model": "biome_vote_losers:block/salt_side"
      },
      "when": {
        "north": "side|up"
      }
    },
    {
      "apply": {
        "model": "biome_vote_losers:block/salt_side_alt"
      },
      "when": {
        "south": "side|up"
      }
    },
    {
      "apply": {
        "model": "biome_vote_losers:block/salt_side_alt",
        "y": 270
      },
      "when": {
        "east": "side|up"
      }
    },
    {
      "apply": {
        "model": "biome_vote_losers:block/salt_side",
        "y": 270
      },
      "when": {
        "west": "side|up"
      }
    },
    {
      "apply": {
        "model": "biome_vote_losers:block/salt_up"
      },
      "when": {
        "north": "up"
      }
    },
    {
      "apply": {
        "model": "biome_vote_losers:block/salt_up",
        "y": 90
      },
      "when": {
        "east": "up"
      }
    },
    {
      "apply": {
        "model": "biome_vote_losers:block/salt_up",
        "y": 180
      },
      "when": {
        "south": "up"
      }
    },
    {
      "apply": {
        "model": "biome_vote_losers:block/salt_up",
        "y": 270
      },
      "when": {
        "west": "up"
      }
    }
  ]
}